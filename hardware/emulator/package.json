{
  "name": "emulator",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "electron-is-dev": "^2.0.0",
    "ipc": "^0.0.1",
    "minim.node": "link:./native/build/Release/minim.node",
    "node-addon-api": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.12.1",
    "react-router-dom": "^6.12.1",
    "react-scripts": "5.0.1"
  },
  "build": {
    "appId": "S/W/D/2",
    "mac": {
      "category": "audio"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose",
    "eject": "react-scripts eject",
    "install": "node-gyp rebuild -C ./native --runtime=electron --target=25.0.0 --arch=x64 --dist-url=https://electronjs.org/headers",
    "dev": "concurrently -k \"BROWSER=none yarn start\" \"yarn:electron\"",
    "electron": "wait-on tcp:127.0.0.1:3000 && electron .",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.10",
    "cmake-js": "^7.2.1",
    "concurrently": "^8.1.0",
    "electron": "^25.2.0",
    "electron-builder": "^24.4.0",
    "jest": "^27.5.1",
    "node-gyp": "^9.4.0",
    "wait-on": "^7.0.1"
  }
}
